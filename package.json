{
  "name": "caoutchouc",
  "version": "1.6.0",
  "description": "A Set of Mootools UI Components",
  "homepage": "https://github.com/codepolitan/caoutchouc#readme",
  "license": "MIT",
  "author": "Code Politan <codepolitan@gmail.com>",
  "contributors": [
    "Bruno Santos <santos@evok.com>"
  ],
  "bugs": {
    "url": "https://github.com/codepolitan/caoutchouc/issues"
  },
  "main": "index.js",
  "scripts": {
    "uglify": "uglifyjs-folder dist -eo dist",
    "build:js": "rm -rf dist && webpack -p && git add -f dist/* && git commit -am 'build'",
    "build:less": "npm run build:less:material && npm run build:less:floor && npm run build:less:print",
    "build:less:material": "lessc src/skin/material/material.less dist/skin/material.css",
    "build:less:floor": "lessc src/skin/floor/floor.less dist/skin/floor.css",
    "build:less:print": "lessc src/skin/base/print.less dist/skin/print.css",
    "watch": "watch 'npm run build:less' src/skin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codepolitan/caoutchouc.git"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "raw-loader": "^0.5.1",
    "script-loader": "^0.7.0",
    "watch": "^1.0.1",
    "webpack": "^1.13.2"
  }
}
