{
  "name": "caoutchouc",
  "version": "1.8.0",
  "description": "A collection of Mootools UI Components",
  "homepage": "https://github.com/codepolitan/caoutchouc#readme",
  "license": "MIT",
  "author": "Code Politan <codepolitan@gmail.com>",
  "contributors": [
    "Bruno Santos <santos@evok.com>"
  ],
  "bugs": {
    "url": "https://github.com/codepolitan/caoutchouc/issues"
  },
  "main": "index.js",
  "scripts": {
    "preinstall": "rm -rf node_modules && mkdir node_modules && for i in js-debugger minimal-languages minimal-utils minimal-binding mootools-pack; do if [ -d ~/node_modules/$i ]; then ln -s ~/node_modules/$i node_modules; fi; done",
    "install": "git clone https://github.com/darkwing/ScrollSpy.git -b 1.5 node_modules/ScrollSpy",
    "build:dev": "rm -rf dist && npm run build:css && npm run build:vendor && webpack --watch",
    "build:prod": "rm -rf dist && npm run build:css && npm run build:vendor && webpack --prod",
    "build:vendor": "cp -r src/font dist/font && cp -r src/icon dist/icon",
    "build:css": "npm run build:css:material && npm run build:css:floor && npm run build:css:print",
    "build:css:material": "lessc src/skin/material/material.less dist/skin/material.css",
    "build:css:floor": "lessc src/skin/floor/floor.less dist/skin/floor.css",
    "build:css:print": "lessc src/skin/base/print.less dist/skin/print.css",
    "add:dist": "git add -f dist/* && git commit -am 'build'",
    "watch": "watch 'npm run build:css' src/skin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codepolitan/caoutchouc.git"
  },
  "files": [
    "dist",
    "docs"
  ],
  "dependencies": {
    "minimal-languages": "git+ssh://git@git.evok.ch:evok-dev/minimal-languages.git#release-1.3",
    "minimal-utils": "git+ssh://git@git.evok.ch:evok-dev/minimal-utils.git#release-1.2",
    "minimal-binding": "git+ssh://git@git.evok.ch:evok-dev/minimal-binding.git#release-1.2",
    "mootools-pack": "git+ssh://git@git.evok.ch:evok-dev/mootools-pack.git#develop",
    "js-debugger": "https://github.com/PertapaCode/js-debugger.git#develop",
    "moment": "https://github.com/moment/moment.git#2.5.1",
    "ckeditor": "https://github.com/ckeditor/ckeditor-releases.git#full/4.5.9",
    "codemirror": "https://github.com/codemirror/CodeMirror.git#3.19.0",
    "mustache.js": "https://github.com/janl/mustache.js.git#v2.1.3",
    "script.js": "https://github.com/ded/script.js.git#v2.5.8"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "imports-loader": "^0.6.5",
    "raw-loader": "^0.5.1",
    "script-loader": "^0.7.0",
    "watch": "^1.0.1",
    "webpack": "^1.13.2"
  }
}
